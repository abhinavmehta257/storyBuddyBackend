{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "singularName": "story",
    "pluralName": "stories",
    "displayName": "story"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "cover": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "genre": {
      "type": "string"
    },
    "isPublic": {
      "type": "boolean"
    },
    "creator": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "stories"
    }
  }
}
